<view class="body"><form data-event-opts="{{[['submit',[['Regist',['$event']]]],['reset',[['',['$event']]]]]}}" bindsubmit="__e" bindreset="__e"><view class="user"><image class="Icon" src="../../static/user.png"></image><input class="userName" name="userName" type="text" placeholder="请输入用户名" data-event-opts="{{[['input',[['__set_model',['','userName','$event',[]]]]]]}}" value="{{userName}}" bindinput="__e"/></view><view class="pwd"><image class="Icon" src="../../static/pwd.png"></image><input class="userPwd" name="userPwd" type="password" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','userPwd','$event',[]]]]]]}}" value="{{userPwd}}" bindinput="__e"/></view><view class="pwd"><image class="Icon" src="../../static/pwd.png" mode></image><input class="userPwd2" name="userPwd2" type="password" placeholder="请再次输入密码" data-event-opts="{{[['input',[['__set_model',['','userPwd2','$event',[]]]]]]}}" value="{{userPwd2}}" bindinput="__e"/></view><view class="tele"><image class="Icon" src="../../static/pwd.png" mode></image><input class="Phone" name="Phone" type="number" maxlength="11" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','Phone','$event',[]]]]]]}}" value="{{Phone}}" bindinput="__e"/></view><button class="btn_submit" style="height:80rpx;" form-type="submit" disabled="{{disabled}}" loading="{{loading}}">{{registText}}</button></form></view>