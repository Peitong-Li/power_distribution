<view class="body"><form data-event-opts="{{[['submit',[['Login',['$event']]]],['reset',[['',['$event']]]]]}}" bindsubmit="__e" bindreset="__e"><view class="user"><image class="Icon" src="../../static/user.png"></image><input id="userName" name="userName" type="text" placeholder="用户名" data-event-opts="{{[['input',[['__set_model',['','userName','$event',[]]]]]]}}" value="{{userName}}" bindinput="__e"/></view><view class="pwd"><image class="Icon" src="../../static/pwd.png"></image><input id="userPwd" name="userPwd" type="password" placeholder="密码" data-event-opts="{{[['input',[['__set_model',['','userPwd','$event',[]]]]]]}}" value="{{userPwd}}" bindinput="__e"/></view><button class="btn_submit" style="height:80rpx;" form-type="submit" disabled="{{disabled}}" loading="{{loading}}">{{loginText}}</button></form><view class="text"><text class="forget">忘记密码</text><text data-event-opts="{{[['tap',[['toRegist',['$event']]]]]}}" class="regist" bindtap="__e">注册</text></view></view>